<div class="modal fade" id="createAcctDialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Create Account</h4>
      </div>
      <div class="modal-body">
        <form class="form-horizontal">
          <div class="form-group">
            <label for="createUsername" class="col-lg-2 control-label">Username</label>
            <div class="col-lg-10">
              <input type="text" class="form-control" id="createUsername" placeholder="Username" required>
            </div>
          </div>
          <div class="form-group">
            <label for="createEmail" class="col-lg-2 control-label">Email</label>
            <div class="col-lg-10">
              <input type="email" class="form-control" id="createEmail" placeholder="Email" required>
            </div>
          </div>
          <div class="form-group">
            <label for="createPassword" class="col-lg-2 control-label">Password</label>
            <div class="col-lg-10">
              <input type="password" class="form-control" id="createPassword" placeholder="Password" required>
            </div>
          </div>
          <div class="form-group">
            <label for="createPasswordConfirm" class="col-lg-2 control-label">Confirm Password</label>
            <div class="col-lg-10">
              <input type="password" class="form-control" id="createPasswordConfirm" placeholder="Confirm Password" required>
            </div>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" id='createSubmit'>Create</button>
      </div>
    </div>
  </div>
</div>